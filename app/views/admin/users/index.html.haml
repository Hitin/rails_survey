%table
  %tr
    %td ID
    %td First name
    %td Last name
    %td Type
    %td Email
    %td Phone
    %td Action
  - @users.each do |user|
    %tr
      %td= link_to user.id, admin_user_path(user.id)
      %td= user.first_name
      %td= user.last_name
      %td= user.type
      %td= user.email
      %td= user.phone
      %td= link_to 'Edit', edit_admin_user_path(user.id)

